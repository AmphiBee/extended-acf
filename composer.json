{
    "name": "wordplate/acf",
    "description": "An Advanced Custom Fields helper for WordPlate",
    "keywords": [
        "wordplate",
        "acf",
        "wordpress",
        "advanced",
        "custom",
        "fields"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se",
            "homepage": "https://doubledip.se"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se",
            "homepage": "https://puredazzle.se"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WordPlate\\Acf\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WordPlate\\Tests\\Acf\\": "tests/"
        },
        "files": [
            "tests/stubs/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
